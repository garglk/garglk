#include <tads.h>

f1(a, b, c?, d?)
{
    "f1: a=<<a>>, b=<<b>>, c=<<c>>\n";
}

f2(a, b, c = 'default c', d?)
{
    "f2: a=<<a>>, b=<<b>>, c=<<c>>, d=<<d>>\n";
}

f3(a, b, c?)
{
    "f3: a=<<a>>, b=<<b>>, c=<<c>>\n";
}

main(args)
{
    f1(1);
    f1(1, 2);
    f1(1, 2, 3);
    f1(1, 2, 3, 4);
    f1(1, 2, 3, 4, 5);

    f2(1);
    f2(1, 2);
    f2(1, 2, 3);
    f2(1, 2, 3, 4);
    f2(1, 2, 3, 4, 5);

    f3(1);
    f3(1, 2);
    f3(1, 2, 3);
    f3(1, 2, 3, 4);
}
