#include <tads.h>

f1(a, b, c?)
{
    "f1: a=<<a>>, b=<<b>>, c=<<c>>\n";
}

f2(a, b, c?, d?)
{
    "f2: a=<<a>>, b=<<b>>, c=<<c>>, d=<<d>>\n";
}

f3(a, b = 'B default', c = 'C default (b=<<b>>)')
{
    "f3: a=<<a>>, b=<<b>>, c=<<c>>\n";
}

n1(a, b:, c:?)
{
    "n1: a=<<a>>, b=<<b>>, c=<<c>>\n";
}

n2(a, b:, c:='C default')
{
    "n2: a=<<a>>, b=<<b>>, c=<<c>>\n";
}

n3(a, b: = 'B default', c: = 'C default (b=<<b>>)')
{
    "n3: a=<<a>>, b=<<b>>, c=<<c>>\n";
}

main(args)
{
    f1(1, 2);
    f1(3, 4, 5);

    f2(6, 7);
    f2(8, 9, 10);
    f2(11, 12, 13, 14);

    f3(15);
    f3(16, nil);
    f3(17, nil, nil);
    f3(18, 19);
    f3(19, 20, nil);
    f3(21, nil, 22);
    f3(23, 24, 25);

    n1(101, b:102);
    n1(103, b:104, c:105);
    
    n2(106, b:107);
    n2(108, b:109, c:110);

    n3(111);
    n3(112, b:nil);
    n3(113, b:114);
    n3(115, b:116, c:nil);
    n3(117, b:118, c:119);
    n3(120, b:nil, c:121);
}
